{
  "name": "chrome-graphql-voyager",
  "description": "Chrome extension for GraphQL Voyager",
  "author": {
    "name": "Nikolay Lanets",
    "email": "n.lanets@newpg.ru"
  },
  "version": "1.1.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts --max-old-space-size=2048 start",
    "build": "webpack",
    "zip": "yarn build && zip -r ../chrome-graphql-voyager.zip manifest.json package.json cgp.html LICENSE README.md cgp.html src/background.js public dist",
    "release": "echo 'Remember to update and stage the version number in manifest.json first!' && yarn version && git push --follow-tags"
  },
  "dependencies": {
    "apollo-link-http": "^1.5.17",
    "cuid": "^2.1.8",
    "graphql-voyager": "^1.0.0-rc.15",
    "react": "^16",
    "react-dom": "^16"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "graphql": "^15.3.0",
    "jquery": "^3.2.1",
    "react-scripts": "^3.4.3",
    "style-loader": "^0.21.0",
    "svg-inline-loader": "^0.8.0",
    "webpack": "^3.8.1",
    "webpack-cli": "^3.3.12"
  },
  "resolutions": {
    "cuid": "2.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
